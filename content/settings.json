{
  "siteMetadata": {
    "language": "en",
    "siteUrl": "https://gatsby-starter-portfolio-minimal-theme.netlify.app/",
    "title": "Smolthing",
    "titleTemplate": "%s Â· Portfolio",
    "description": "An everything website",
    "author": "Smolthing",
    "avatar": "../content/images/gatsby.png",
    "bio": "I write everything here.",
    "social": {
      "blog": "/"
    }
  },
  "siteConfiguration": {
    "logo": { "text": "Smolthing" },
    "navigation": {
      "header": [
        { "label": "Blog", "url": "/blog" },
        { "label": "Apps", "url": "/#apps" },
        { "label": "Contact", "url": "/#contact" }
      ],
      "ctaButton": {
        "openNewTab": true,
        "label": "Youtube",
        "url": "https://youtube.com"
      },
      "footer": [
        { "label": "Privacy", "url": "/privacy" },
        { "label": "About", "url": "/about" }
      ]
    },
    "featureToggles": {
      "useDarkModeAsDefault": false,
      "useDarkModeBasedOnUsersPreference": true,
      "useCookieBar": false
    }
  }
}
